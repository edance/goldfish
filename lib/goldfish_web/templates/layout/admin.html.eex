  </head>

  <body class="header" >
    <%= render GoldfishWeb.SharedView, "nav.html", assigns %>

    <main role="main">
      <%= render @view_module, @view_template, assigns %>
    </main>

    <script src="<%= Routes.static_path(@conn, "/js/admin.js") %>"></script>
  </body>
</html>
